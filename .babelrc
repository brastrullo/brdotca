{
  "presets": [
    [{
      "env": {
        "firefox": {
          "sourceMaps": "inline",
          "blacklist": [
            "es5.properties.mutators",
            "es6.arrowFunctions",
            "es6.destructuring",
            "es6.forOf",
            "es6.parameters",
            "es6.properties.computed",
            "es6.properties.shorthand",
            "es6.spec.symbols",
            "es6.spread",
            "es6.tailCall",
            "es6.templateLiterals",
            "es6.regex.sticky",
            "es6.regex.unicode"
          ]
        }
      }
    }]
  ]
}